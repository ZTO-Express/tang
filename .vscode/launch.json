{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Lib Build",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/sucrase-node",
      "runtimeArgs": ["../../scripts/build", "--name=zpage"],
      "cwd": "${workspaceFolder}/packages/zpage"
    },
    {
      "name": "Launch UI Element Build",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/sucrase-node",
      "runtimeArgs": ["../../scripts/build", "--name=ui-element"],
      "cwd": "${workspaceFolder}/packages/ui-element"
    },
    {
      "name": "Gen Version",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/sucrase-node",
      "runtimeArgs": ["scripts/version/gen-version.ts"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
