{
  "extends": "../../tsconfig.build.json",
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "target": "es6",
    "declaration": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": false
  },
  "include": ["src"],
  "exclude": ["dist"]
}
