{
  "name": "tang-yapi-sharing",
  "version": "0.0.1",
  "title": "yapi文档生成",
  "description": "此预设用于通过api文档生成vue项目、调用接口及页面",
  "contact": {
    "name": "rayl",
    "url": "https://github.com/ZTO-Express/tang",
    "email": "rayl@pisaas.com"
  },
  "extends": "",
  "plugins": [
    {
      "name": "openapi-titan",
      "install": [
        "git clone git@github.com:pient/devspace.git",
        "git checkout dev"
      ]
    },
    {
      "name": "yapi-openapi",
      "install": "npm install",
      "version": "0.0.1"
    },
    {
      "name": "fsharing",
      "install": "npm install",
      "version": "0.0.1"
    }
  ],
  "loaders": [],
  "parsers": [
    {
      "parser": "plugin:yapi-openapi/parser",
      "options": {}
    },
    {
      "parser": "plugin:openapi-titan/parser",
      "options": {}
    }
  ],
  "generators": [
    {
      "generator": "plugin:openapi-titan/generator",
      "options": {
        "include": [],
        "exclude": [],
        "output": {
          "name": "local",
          "overwrite": true
        }
      }
    },
    {
      "generator": "plugin:yapi-openapi/generator",
      "options": {
        "output": {
          "name": "github",
          "overwrite": true
        }
      }
    }
  ],
  "outputers": [
    {
      "name": "local",
      "outputDir": "./out"
    },
    {
      "name": "github",
      "repository": ""
    }
  ],
  "snippets": [
    {
      "snippet": "plugin:fsharing/snippets"
    }
  ],
  "hooks": {
    "beforeLoad": {
      "name": ["titan:output"]
    }
  }
}
